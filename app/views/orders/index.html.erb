<h1>All Orders</h1>
<%= form_with model: @order do |f| %>
    <%= f.label :Beer_name %>
  <p> <%= f.text_field :beer_name%><p>
    <%= f.label :Description %>
   <p> <%= f.text_field :description%><p>
    <%= f.label :quantity %>
   <p> <%= f.number_field :quantity %><p>
   
    <%= f.submit %>
<% end %>
